LTEXT "Default Release Plane:", -1,10,55,80,10,WS_GROUP
EDITTEXT IDD_DEFRELEASEPLANE_EDIT, 90,55,49,10, WS_GROUP
LTEXT "Default Close Enough:", -1,10,70,80,10,WS_GROUP
